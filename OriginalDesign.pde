  ellipse(x,y,10,10);
  if(key=='a'){
  	x++;//9
  	if(x>90){
  		x=50;
  	}
  }
  if(key=='b'){
  	y++;//5
  	if(y>90){
  		y=50;
  	}
  }
  if(key=='c'){
  	x++;
  	y++;//7
  	if(x>90&&y>90){
